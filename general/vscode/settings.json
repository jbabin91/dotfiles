{
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[mdx]": {
    "editor.codeActionsOnSave": {
      "source.addMissingImports": "never",
      "source.fixAll.eslint": "never",
      "source.removeUnusedImports": "never"
    },
    "editor.formatOnSave": false
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "breadcrumbs.filePath": "on",
  "breadcrumbs.symbolPath": "off",
  "cSpell.allowCompoundWords": true,
  "cSpell.diagnosticLevel": "Warning",
  "cSpell.ignorePaths": [
    "package-lock.json",
    "pnpm-lock.yaml",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders"
  ],
  "cSpell.language": "en,en-US",
  "cSpell.userWords": [],
  "catppuccin-icons.hidesExplorerArrows": true,
  "catppuccin.extraBordersEnabled": true,
  "colorize.languages": ["javascript", "typescript", "javascriptreact", "typescriptreact"],
  "console-ninja.featureSet": "Community",
  "console-ninja.showWhatsNew": false,
  "diffEditor.maxComputationTime": 0,
  "dotenv.enableAutocloaking": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "explicit",
    "source.fixAll.eslint": "explicit",
    "source.removeUnusedImports": "explicit"
  },
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "'MonoLisa Variant', 'Dank Mono', 'Cascadia Code', 'Victor Mono', Menlo, Monaco, 'Courier New', monospace, 'Symbols Nerd Font Mono'",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.bracketPairsHorizontal": true,
  "editor.guides.highlightActiveBracketPair": true,
  "editor.guides.highlightActiveIndentation": true,
  "editor.guides.indentation": true,
  "editor.linkedEditing": true,
  "editor.matchBrackets": "never",
  "editor.minimap.enabled": false,
  "editor.parameterHints.enabled": true,
  "editor.quickSuggestions": {
    "comments": false,
    "other": false,
    "strings": true
  },
  "editor.renderWhitespace": "boundary",
  "editor.semanticHighlighting.enabled": true,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": false,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "[*Dark*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#fff"
          }
        }
      ]
    },
    "[*Light*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#000"
          }
        }
      ]
    },
    "textMateRules": []
  },
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.wordBasedSuggestions": "matchingDocuments",
  "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 100,
  "emmet.includeLanguages": {
    "astro": "astro",
    "ejs": "html",
    "erb": "erb",
    "html": "html",
    "javascript": "javascriptreact",
    "typescript": "typescriptreact",
    "vue": "html"
  },
  // "emmet.showAbbreviationSuggestions": false,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "errorLens.enabledDiagnosticLevels": ["warning", "info", "error"],
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": true,
  "explorer.fileNesting.patterns": {
    "*.asax": "$(capture).*.cs, $(capture).*.vb",
    "*.ascx": "$(capture).*.cs, $(capture).*.vb",
    "*.ashx": "$(capture).*.cs, $(capture).*.vb",
    "*.aspx": "$(capture).*.cs, $(capture).*.vb",
    "*.bloc.dart": "$(capture).event.dart, $(capture).state.dart",
    "*.c": "$(capture).h",
    "*.cc": "$(capture).hpp, $(capture).h, $(capture).hxx",
    "*.cjs": "$(capture).cjs.map, $(capture).*.cjs, $(capture)_*.cjs",
    "*.component.ts": "$(capture).component.html, $(capture).component.spec.ts, $(capture).component.css, $(capture).component.scss, $(capture).component.sass, $(capture).component.less",
    "*.cpp": "$(capture).hpp, $(capture).h, $(capture).hxx",
    "*.cs": "$(capture).*.cs",
    "*.cshtml": "$(capture).cshtml.cs",
    "*.csproj": "*.config, *proj.user, appsettings.*, bundleconfig.json",
    "*.css": "$(capture).css.map, $(capture).*.css",
    "*.cxx": "$(capture).hpp, $(capture).h, $(capture).hxx",
    "*.dart": "$(capture).freezed.dart, $(capture).g.dart",
    "*.ex": "$(capture).html.eex, $(capture).html.heex, $(capture).html.leex",
    "*.go": "$(capture)_test.go",
    "*.java": "$(capture).class",
    "*.js": "$(capture).module.css, $(capture).module.scss, $(capture).stories.jsx, $(capture).stories.mdx, $(capture).test.js, $(capture).spec.js, $(capture).mocks.js",
    "*.jsx": "$(capture).module.css, $(capture).module.scss, $(capture).stories.jsx, $(capture).stories.mdx, $(capture).test.js, $(capture).test.jsx, $(capture).spec.js, $(capture).spec.js, $(capture).mocks.js",
    "*.master": "$(capture).*.cs, $(capture).*.vb",
    "*.mjs": "$(capture).mjs.map, $(capture).*.mjs, $(capture)_*.mjs",
    "*.module.ts": "$(capture).resolver.ts, $(capture).controller.ts, $(capture).service.ts",
    "*.pubxml": "$(capture).pubxml.user",
    "*.resx": "$(capture).*.resx, $(capture).designer.cs, $(capture).designer.vb",
    "*.tex": "$(capture).acn, $(capture).acr, $(capture).alg, $(capture).aux, $(capture).bbl, $(capture).blg, $(capture).fdb_latexmk, $(capture).fls, $(capture).glg, $(capture).glo, $(capture).gls, $(capture).idx, $(capture).ind, $(capture).ist, $(capture).lof, $(capture).log, $(capture).lot, $(capture).out, $(capture).pdf, $(capture).synctex.gz, $(capture).toc, $(capture).xdv",
    "*.ts": "$(capture).mdx, $(capture).*.mdx, $(capture).module.css, $(capture).module.scss, $(capture).stories.tsx, $(capture).stories.mdx, $(capture).test.ts, $(capture).spec.ts, $(capture).mocks.ts",
    "*.tsx": "$(capture).mdx, $(capture).*.mdx, $(capture).module.css, $(capture).module.scss, $(capture).stories.tsx, $(capture).stories.mdx, $(capture).test.ts, $(capture).test.tsx, $(capture).spec.ts, $(capture).spec.tsx, $(capture).mocks.ts",
    "+layout.svelte": "+layout.ts,+layout.ts,+layout.js,+layout.server.ts,+layout.server.js,+layout.gql",
    "+page.svelte": "+page.server.ts,+page.server.js,+page.ts,+page.js,+page.gql",
    ".env": "*.env, .env.*, .envrc, env.d.ts",
    ".env.*": ".env.local.*",
    ".eslintrc": ".eslint*",
    ".eslintrc.*": ".eslint*",
    ".gitignore": ".gitattributes, .gitmodules, .gitmessage, .mailmap, .git-blame*",
    ".prettierrc": ".prettier*, prettier*",
    ".prettierrc.*": ".prettier*, prettier*",
    "Dockerfile": "Dockerfile.*",
    "astro.config.*": "*.env, .babelrc*, .codecov, .cssnanorc*, .env.*, .envrc, .htmlnanorc*, .lighthouserc.*, .mocha*, .postcssrc*, .terserrc*, api-extractor.json, ava.config.*, babel.config.*, contentlayer.config.*, cssnano.config.*, cypress.*, env.d.ts, formkit.config.*, formulate.config.*, histoire.config.*, htmlnanorc.*, jasmine.*, jest.config.*, jsconfig.*, karma*, lighthouserc.*, playwright.config.*, postcss.config.*, puppeteer.config.*, rspack.config.*, svgo.config.*, tailwind.config.*, tsdoc.*, uno.config.*, unocss.config.*, vitest.config.*, webpack.config.*, windi.config.*",
    "cargo.toml": ".clippy.toml, .rustfmt.toml, cargo.lock, clippy.toml, cross.toml, rust-toolchain.toml, rustfmt.toml",
    "compose.yaml": "compose.*",
    "default.nix": "shell.nix",
    "docker-compose.*": "docker-compose.*",
    "dockerfile": ".dockerignore, docker-compose.*, compose.*, dockerfile*",
    "eslint.config.*": ".eslint*",
    "nx.*": "project*",
    "package.json": ".pnpm*, .yarnrc*, package-lock.json, pnpm*, yarn*, bun*, .nvmrc, .npmrc",
    "readme*": "authors, backers*, changelog*, citation*, code_of_conduct*, codeowners, contributing*, contributors, copying, credits, governance.md, history.md, license*, maintainers, readme*, security.md, sponsors*",
    "tsconfig.json": "tsconfig.*.json",
    "webpack.config.js": "webpack.*.js"
  },
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.css": "tailwindcss",
    ".env*": "dotenv"
  },
  "files.enableTrash": false,
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    ".figmagic": true,
    ".nx": true,
    ".output": true,
    "coverage": true,
    "tmp": true
  },
  "files.trimTrailingWhitespace": true,
  "git.mergeEditor": true,
  "git.openRepositoryInParentFolders": "always",
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.editor.enableCodeActions": false,
  "gitlens.defaultGravatarsStyle": "retro",
  "gitlens.graph.minimap.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.mode.statusBar.enabled": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "html.autoClosingTags": true,
  "i18n-ally.displayLanguage": "en",
  "javascript.autoClosingTags": true,
  "javascript.preferences.importModuleSpecifier": "shortest",
  "javascript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "json.format.enable": false,
  "markdownlint.config": {
    "MD001": false,
    "MD012": false,
    "MD024": false,
    "MD025": false,
    "MD026": false,
    "MD029": false,
    "MD033": false,
    "MD036": false,
    "MD041": false
  },
  "material-icon-theme.hidesExplorerArrows": true,
  "mdx.server.enable": true,
  "prettier.documentSelectors": ["**/*.astro"],
  "react-native-tools.showUserTips": false,
  "redhat.telemetry.enabled": false,
  "search.exclude": {
    "**/.git": true,
    "**/.github": true,
    "**/.nuxt": true,
    "**/.output": true,
    "**/.pnpm": true,
    "**/.vscode": true,
    "**/.yarn": true,
    "**/bower_components": true,
    "**/build": true,
    "**/dist": true,
    "**/dist/**": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/out/**": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/tmp": true,
    "**/yarn.lock": true,
    ".DS_Store": true,
    ".git": true,
    ".pnp.cjs": true,
    ".pnp.js": true,
    ".pnp.mjs": true,
    ".vscode": true,
    ".yarn": true
  },
  "search.smartCase": true,
  "search.useIgnoreFiles": true,
  "supermaven.otherWarning": false,
  "svelte.enable-ts-plugin": true,
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.experimental.classRegex": [["tv\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"]],
  "tailwindCSS.validate": true,
  "terminal.integrated.env.osx": {
    "CW_NEW_SESSION": "1",
    "FIG_NEW_SESSION": "1",
    "Q_NEW_SESSION": "1"
  },
  "terminal.integrated.minimumContrastRatio": 2,
  "typescript.autoClosingTags": true,
  "typescript.preferences.importModuleSpecifier": "shortest",
  "typescript.preferences.importModuleSpecifierEnding": "minimal",
  "typescript.suggest.autoImports": true,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.surveys.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.commandCenter": false,
  "workbench.colorTheme": "Catppuccin Macchiato",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.customLabels.enabled": true,
  "workbench.editor.customLabels.patterns": {
    "**/app/**/index.tsx": "${dirname} - Index",
    "**/app/**/layout.tsx": "${dirname} - Layout",
    "**/app/**/page.ts": "${dirname} - Page",
    "**/app/**/template.tsx": "${dirname} - Template",
    "**/components/**/index.tsx": "${dirname} - Component"
  },
  "workbench.editor.decorations.colors": true,
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.value": 6,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.settings.editor": "json",
  "workbench.tree.enableStickyScroll": false,
  "workbench.tree.indent": 10
}
