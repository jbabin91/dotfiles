{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "caseSensitive": false,
  "allowCompoundWords": true,
  "dictionaryDefinitions": [
    {
      "name": "personal-words",
      "path": "./.cspell/personal-words.txt",
      "addWords": true
    },
    {
      "name": "misc-words",
      "path": "./.cspell/misc.txt",
      "addWords": true
    }
  ],
  "dictionaries": [
    "personal-words",
    "misc-words",
    "aws",
    "bash",
    "coding-compound-terms",
    "companies",
    "computing-acronyms",
    "cpp",
    "csharp",
    "css",
    "dart",
    "docker",
    "dotnet",
    "en-common-misspellings",
    "filetypes",
    "fonts",
    "fullstack",
    "game-development",
    "gaming-terms",
    "git",
    "golang",
    "google",
    "html",
    "html-symbol-entities",
    "latex",
    "lorem-ipsum",
    "lua",
    "makefile",
    "node",
    "npm",
    "python",
    "rust",
    "shellscript",
    "software-term-suggestions",
    "software-tools",
    "softwareTerms",
    "sql",
    "svelte",
    "terraform",
    "typescript",
    "web-services"
  ],
  "ignorePaths": [
    "node_modules",
    "package-lock.json",
    "pnpm-lock.yaml",
    "/personal-words.txt",
    "/misc.txt"
  ]
}
